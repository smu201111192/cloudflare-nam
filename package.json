{
  "name": "hono-minimal",
  "private": true,
  "version": "0.0.1",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "test": "jest --verbose",
    "dev": "npx wrangler dev",
    "dev:remote": "wrangler dev --remote",
    "deploy": "wrangler deploy src/index.ts --env production",
    "upload": "node --loader ts-node/esm src/scripts/upload",
    "connect-local": "pscale connect cloudflare-lol-db develop --port 53136",
    "db:push": "dotenv -e .dev.env -- npx prisma db push",
    "db:type:gen": "npx supabase gen types typescript --local > ./src/db/supabase.ts",
    "db:diff": "npx supabase db diff --local --use-migra -f schema_update"
  },
  "license": "MIT",
  "dependencies": {
    "@planetscale/database": "^1.10.0",
    "@prisma/client": "^5.3.1",
    "@prisma/extension-accelerate": "^0.5.0",
    "@supabase/supabase-js": "^2.38.0",
    "browser-env": "^3.3.0",
    "Buffer": "^0.0.0",
    "camelcasejs": "^1.0.3",
    "dotenv": "^8.1.0",
    "hono": "^2.5.7",
    "http-status": "^1.7.0",
    "http-status-codes": "^1.3.2",
    "ky": "^1.0.1",
    "kysely": "^0.26.1",
    "kysely-d1": "^0.3.0",
    "kysely-planetscale": "^1.4.0",
    "lodash": "^4.17.15",
    "prisma-kysely": "^1.5.0",
    "querystring": "^0.2.1",
    "ts-node": "^10.9.1",
    "undici": "^5.23.0",
    "uuid": "^8.3.0",
    "zod-error": "^1.5.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.421.0",
    "@cloudflare/workers-types": "^4.20221111.1",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.0",
    "@types/node-fetch": "^2.6.6",
    "@types/p-queue": "^3.2.1",
    "better-sqlite3": "^8.0.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "esbuild": "^0.15.15",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.3.1",
    "jest-environment-miniflare": "^2.11.0",
    "miniflare": "^3.20231010.0",
    "prisma": "^5.1.1",
    "typescript": "^5.0.4",
    "wrangler": "^3.12.0"
  }
}
